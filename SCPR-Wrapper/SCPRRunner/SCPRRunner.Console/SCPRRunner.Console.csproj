<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="28.0.1" />
    <PackageReference Include="HEAL.SCS" Version="1.0.2" />
    <PackageReference Include="HEAL.SCS.win-x64" Version="1.0.2" />
    <PackageReference Include="Mosek" Version="9.2.49" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="ShapeConstrainedPolynomialRegression">
      <HintPath>ShapeConstrainedPolynomialRegression.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="alglibnet2.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="ShapeConstrainedPolynomialRegression.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="ShapeConstrainedPolynomialRegression.pdb">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
